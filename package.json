{
  "name": "@omniaura/solid-hotkeys",
  "version": "0.1.0",
  "description": "SolidJS adapter for TanStack Hotkeys - keyboard shortcuts made easy",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test --conditions=browser",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "solid",
    "solidjs",
    "hotkeys",
    "keyboard",
    "shortcuts",
    "tanstack",
    "keybindings"
  ],
  "license": "MIT",
  "author": "omniaura",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omniaura/solid-hotkeys.git"
  },
  "homepage": "https://github.com/omniaura/solid-hotkeys#readme",
  "bugs": {
    "url": "https://github.com/omniaura/solid-hotkeys/issues"
  },
  "peerDependencies": {
    "solid-js": ">=1.7.0"
  },
  "dependencies": {
    "@tanstack/hotkeys": "^0.1.0"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^16.0.0",
    "@types/bun": "latest",
    "semantic-release": "25",
    "solid-js": "^1.9.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
